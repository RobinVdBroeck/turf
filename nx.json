{
  "namedInputs": {
    "default": [
      "{workspaceRoot}/package-lock.json",
      "{workspaceRoot}/tsconfig.shared.json",
      "{workspaceRoot}/tsup.config.ts",
      "{projectRoot}/package.json",
      "{projectRoot}/tsconfig.json"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "inputs": [
        "default",
        "{projectRoot}/index.{js,ts}",
        "{projectRoot}/index.d.ts",
        "{projectRoot}/lib/**",
        "{projectRoot}/rollup.config.js",
        "{projectRoot}/babel.config.json"
      ],
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "test": {
      "inputs": [
        "default",
        "{projectRoot}/test.{js,ts}",
        "{projectRoot}/test/**",
        "{projectRoot}/types.ts"
      ],
      "dependsOn": [
        "build"
      ],
      "cache": true
    },
    "last-checks": {
      "inputs": [
        "default",
        "{projectRoot}/test.ts"
      ],
      "dependsOn": [
        "build",
        "^last-checks"
      ],
      "cache": true
    }
  }
}
